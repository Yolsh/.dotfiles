    "network_config": {
        "type": "nm"
    },
    "ntp": true,
    "packages": [
        "ghostty",
        "niri",
        "neovim",
        "pavucontrol",
        "thunar",
        "starship",
        "wl-clipboard",
        "waybar",
        "ttf-jetbrains-mono-nerd",
        "brightnessctl",
        "git"
    ],
    "parallel_downloads": 0,
    "profile_config": {
        "gfx_driver": null,
        "greeter": null,
        "profile": {}
